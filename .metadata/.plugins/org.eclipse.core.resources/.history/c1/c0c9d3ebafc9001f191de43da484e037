package quiz01;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner scan= new Scanner(System.in);
		int N = scan.nextInt();
		
		String str=""; // 빈 문자열 초기화
		
		for(int i=1; i<=N/4; i++) {
			
			
			str+="long ";
			
		}	
		System.out.println(str+"int");//		
		
				
		
	 scan.close();

	
	}
}


